<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="apps-developer-menu" label="Developer">
  <item label="Geany">
    <action name="Execute">
      <command>geany</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

</menu>

<menu id="apps-term-menu" label="Terminals">
  <item label="Xterm">
    <action name="Execute">
      <command>xterm -bg "black" -u8 -fg "white" -fa 'Hack-Regular' -fs '10' -fullscreen</command>
    </action>
  </item>
  <item label="Lxterminal">
    <decor>no</decor>
    <action name="Execute">
      <command>lxterminal</command>
    </action>
  </item>
</menu>

<menu id="apps-net-menu" label="Internet">
  <item label="Firefox">
    <action name="Execute">
      <command>firefox --private</command>
      <startupnotify>
        <enabled>yes</enabled>
        <wmclass>Firefox</wmclass>
      </startupnotify>
    </action>
  </item>

  <item label="Chromium">
    <action name="Execute">
      <command>chromium --incognito</command>
      <startupnotify>
        <enabled>yes</enabled>
        <wmclass>Chromium</wmclass>
      </startupnotify>
    </action>
  </item>

  <item label="Skype">
    <action name="Execute">
      <command>skypeforlinux</command>
      <startupnotify>
        <enabled>yes</enabled>
        <wmclass>Skype</wmclass>
      </startupnotify>
    </action>
  </item>

  <item label="Transmission">
    <action name="Execute">
      <command>transmission-gtk</command>
      <startupnotify>
        <enabled>yes</enabled>
        <wmclass>Transmission</wmclass>
      </startupnotify>
    </action>
  </item>
  <item label="XChat">
    <action name="Execute">
      <command>xchat</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
</menu>

<menu id="apps-multimedia-menu" label="Multimedia">
  <item label="Vlc">
    <action name="Execute">
      <command>vlc</command>
    </action>
  </item>

	<item label="Audacity">
    <action name="Execute">
      <command>audacity</command>
    </action>
  </item>

	<item label="OpenShot">
    <action name="Execute">
      <command>openshot-qt</command>
    </action>
  </item>

	<item label="Pavucontrol">
    <action name="Execute">
      <command>pavucontrol</command>
    </action>
  </item>

</menu>

<menu id="apps-fileman-menu" label="File Managers">
   <item label="Thunar">
    <action name="Execute">
      <command>thunar</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
 </menu>

<menu id="apps-graphics-menu" label="Graphics">
  <item label="Gimp">
    <action name="Execute">
      <command>gimp</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Inkscape">
    <action name="Execute">
      <command>inkscape</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Blender3D">
    <action name="Execute">
      <command>blender</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="FreeCAD">
    <action name="Execute">
      <command>freecad</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Simple Screen Recorder">
    <action name="Execute">
      <command>simplescreenrecorder</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
</menu>

<menu id="study-menu" label="Study">
  <item label="Freemind">
    <action name="Execute">
      <command>freemind</command>
    </action>
  </item>
  <item label="Freeplane">
    <action name="Execute">
      <command>freeplane</command>
    </action>
  </item>
  <item label="Mendeley">
    <action name="Execute">
      <command>mendeleydesktop</command>
    </action>
  </item>
</menu>

<menu id="miscellaneous-menu" label="Miscellaneous">
  <item label="Visual Game Boy">
    <action name="Execute">
      <command>visualboyadvance-m</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>

  <item label="Master PDF editor">
    <action name="Execute">
      <command>masterpdfeditor4</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>

  <item label="Android file transfer">
    <action name="Execute">
      <command>android-file-transfer</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>

  <item label="Hexchat">
    <action name="Execute">
      <command>hexchat</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>

  <item label="XFCE power manager">
    <action name="Execute">
      <command>xfce4-power-manager-settings</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>

</menu>

<menu id="system-menu" label="System">
  <item label="Openbox Configuration Manager">
    <action name="Execute">
      <command>obconf</command>
      <startupnotify><enabled>yes</enabled></startupnotify>
    </action>
  </item>
  <separator />
  <item label="Reconfigure Openbox">
    <action name="Reconfigure" />
  </item>
</menu>

<menu id="exit-menu" label="Exit">
  <item label="Lock">
    <action name="Execute">
      <command>i3lock</command>
    </action>
  </item>

  <item label="Log Out">
    <action name="Execute">
      <command>openbox --exit</command>
    </action>
  </item>
  <item label="Shutdown">
    <action name="Execute">
      <command>systemctl poweroff</command>
    </action>
  </item>
  <item label="Restart">
    <action name="Execute">
      <command>systemctl reboot</command>
    </action>
  </item>
  <item label="Suspend">
    <action name="Execute">
      <command>systemctl suspend</command>
    </action>
  </item>
  <item label="Hibernate">
    <action name="Execute">
      <command>systemctl hibernate</command>
    </action>
  </item>
</menu>

<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />
  <menu id="apps-accessories-menu"/>
  <menu id="apps-graphics-menu"/>
  <menu id="apps-net-menu"/>
  <menu id="apps-office-menu"/>
  <menu id="apps-multimedia-menu"/>
  <menu id="apps-term-menu"/>
  <menu id="apps-fileman-menu"/>
  <menu id="apps-developer-menu"/>
  <menu id="study-menu"/>
	<menu id="miscellaneous-menu"/>
  <separator />
  <menu id="exit-menu"/>
  <menu id="system-menu"/>
  </menu>

</openbox_menu>
