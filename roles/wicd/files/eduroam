name = Eduroam (PEAP with MSCHAPv2)
author = Siqueira
version = 1
require username *Username password *Password
protected password *Password
-----
ctrl_interface=/var/run/wpa_supplicant
network={
	ssid="$_ESSID"
	scan_ssid=$_SCAN
	key_mgmt=WPA-EAP
	eap=PEAP
	phase2="auth=MSCHAPV2"
	identity="$_USERNAME"
	password="$_PASSWORD"
}
